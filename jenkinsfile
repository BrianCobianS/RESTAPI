// This shows a simple example of how to archive the build output artifacts.
pipeline {
    agent any
    stages {
        stage ('Starting the webpage') {
            steps {
                sh "cat Versiones.json"
                sh"ls"
                sh"if [ -f/maven-metadata.xml ]; then rm maven-metadata.xml;else echo 'No, no existe'; fi"
                sh "wget http://10.89.110.62:28081/repository/tgcs-maven-snapshot/com/toshibacommerce/ace/ACE3D001/maven-metadata.xml"
                sh "node callpy.js"
                sh "cat Versiones.json"
                sh "npm run dev"        

            }
        }

    }
}